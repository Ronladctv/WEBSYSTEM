{
    "version": 3,
    "builds": [
        {
            "src": "dist/WEBAPP/server/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/WEBAPP/browser/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "dist/WEBAPP/server/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/WEBAPP/browser/$1"
        }
    ]
}